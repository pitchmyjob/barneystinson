container_commands:
  01_requirement:
    command: "pip install -r requirements/staging.txt"
    test: '[ "$DJANGO_SETTINGS_MODULE" = "settings.staging" ]'
  02_wsgipass:
    command: 'echo "WSGIPassAuthorization On" >> ../wsgi.conf'
  03_migrate:
    command: "django-admin.py migrate"
    leader_only: true